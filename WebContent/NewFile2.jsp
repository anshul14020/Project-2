	<tr>
		<td>XIIth Board*</td>
		<td><input type="text"/></td>
	</tr>
	<tr>
		<td>XIIth Marks(%)*</td>
		<td><input type="text"/></td>
	</tr>
	<tr>
		<td>Year Of Passing XIIth*</td>
				<td><select>
				<option value="20151">2015</option>
